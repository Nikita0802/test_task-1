{
	"compilerOptions": {
		"target": "es5",
		"module": "es6",
		"jsx": "react",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"include": [
		"src/**/*.tsx",
		"svg.d.ts"
	],
	"exclude": [
		"node_modules"
	]
}